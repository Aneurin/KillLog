<!--
Combat Parse: Used to scrape information from Combat Messages
-->

<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
	<Script file="CombatParse.lua"/>
	<Frame name="CombatParseFrame" parent="UIParent" hidden="true">
		<Scripts>
			<OnLoad>
				CombatParse_OnLoad();
			</OnLoad>
			<OnEvent>
				CombatParse_OnEvent(event);
			</OnEvent>
		</Scripts>
	</Frame>
</Ui>
